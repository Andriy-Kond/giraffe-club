{
  "printWidth": 100,
  "tabWidth": 2,
  "semi": false,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "useTabs": false,
  "singleQuote": false,
  "jsxSingleQuote": false,

  "trailingComma": "all",
  "proseWrap": "never",

  "htmlWhitespaceSensitivity": "css",
  "insertPragma": false,
  "quoteProps": "as-needed",
  "requirePragma": false,
  "vueIndentScriptAndStyle": false,
  "singleAttributePerLine": false
}
